<view>
    <columns>
        <column title="Código" order="100">
            <widget type="text" fieldname="codagente" onclick="EditAgente" />
        </column>
        <column title="Nombre" order="110">
            <widget type="text" fieldname="nombre" />
        </column>
        <column title="Cargo" order="120">
            <widget type="text" fieldname="cargo" />
        </column>
        <column title="% Com." display="none" order="130">
            <widget type="text" fieldname="porcomision" />
        </column>
        <column title="Dirección" display="none" order="140">
            <widget type="text" fieldname="direccion" />
        </column>
        <column title="Ciudad" display="center" order="150">
            <widget type="text" fieldname="ciudad" />
        </column>
        <column title="Cod. Postal" display="none" order="160">
            <widget type="text" fieldname="codpostal" />
        </column>
        <column title="Provincia" display="center" order="170">
            <widget type="text" fieldname="provincia" />
        </column>
        <column title="Teléfono" order="180">
            <widget type="text" fieldname="telefono" />
        </column>
        <column title="Email" order="190">
            <widget type="text" fieldname="email" />
        </column>
        <column title="Fec. Alta" display="none" order="200">
            <widget type="text" fieldname="f_alta" />
        </column>
        <column title="Fec. Baja" display="center" order="210">
            <widget type="text" fieldname="f_baja" />
        </column>
        <column title="Nacimiento" display="none" order="220">
            <widget type="text" fieldname="f_nacimiento" />
        </column>
        <column title="Seg. Social" display="none" order="230">
            <widget type="text" fieldname="seg_social" />
        </column>
        <column title="Cta. Banco" display="none" order="240">
            <widget type="text" fieldname="banco" />
        </column>
    </columns>
    
    <filters>        
    </filters>
</view>
