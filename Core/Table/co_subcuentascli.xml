<?xml version="1.0" encoding="UTF-8"?>
<!--
    Document   : co_subcuentascli.xml
    Author     : Carlos Garcia Gomez
    Description:
        Estructura de la tabla co_subcuentascli.
-->
<table>
    <column>
        <name>codcliente</name>
        <type>character varying(10)</type>
        <null>YES</null>
    </column>
    <column>
        <name>codejercicio</name>
        <type>character varying(4)</type>
        <null>NO</null>
    </column>
    <column>
        <name>codsubcuenta</name>
        <type>character varying(15)</type>
        <null>NO</null>
    </column>
    <column>
        <name>id</name>
        <type>serial</type>
        <null>NO</null>
        <default>nextval('co_subcuentascli_id_seq'::regclass)</default>
    </column>
    <column>
        <name>idsubcuenta</name>
        <type>integer</type>
        <null>NO</null>
    </column>
    <indexes>
        <name>co_subcuentascli_pkey</name>
        <constraint>PRIMARY KEY (id)</constraint>
    </indexes>
    <indexes>
        <name>ca_co_subcuentascli_ejercicios</name>
        <constraint>FOREIGN KEY (codejercicio)
            REFERENCES ejercicios (codejercicio)
            ON DELETE CASCADE ON UPDATE CASCADE</constraint>
    </indexes>
    <indexes>
        <name>ca_co_subcuentascli_clientes</name>
        <constraint>FOREIGN KEY (codcliente)
            REFERENCES clientes (codcliente)
            ON DELETE CASCADE ON UPDATE CASCADE</constraint>
    </indexes>
    <indexes>
        <name>ca_co_subcuentascli_subcuentas</name>
        <constraint>FOREIGN KEY (idsubcuenta)
            REFERENCES co_subcuentas (idsubcuenta)
            ON DELETE CASCADE ON UPDATE CASCADE</constraint>
    </indexes>
</table>