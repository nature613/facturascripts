<?xml version="1.0" encoding="UTF-8"?>
<!--
    Document   : atributos_valores.xml
    Author     : Carlos Garcia Gomez
    Description:
        Estructura de la tabla atributos_valores.
-->
<table>
    <column>
        <name>id</name>
        <type>serial</type>
        <null>NO</null>
        <default>nextval('atributos_valores_seq'::regclass)</default>
    </column>
    <column>
        <name>codatributo</name>
        <type>character varying(20)</type>
        <null>NO</null>
    </column>
    <column>
        <name>valor</name>
        <type>character varying(100)</type>
        <null>NO</null>
    </column>
    <indexes>
        <name>atributos_valores_pkey</name>
        <constraint>PRIMARY KEY (id)</constraint>
    </indexes>
    <indexes>
        <name>ca_atributos_valores</name>
        <constraint>FOREIGN KEY (codatributo)
            REFERENCES atributos (codatributo)
            ON DELETE CASCADE ON UPDATE CASCADE</constraint>
    </indexes>
</table>