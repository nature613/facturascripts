<?xml version="1.0" encoding="UTF-8"?>
<!--
    Document   : secuencias.xml
    Author     : Carlos Garcia Gomez
    Description:
        Estructura de la tabla secuencias.
-->
<table>
    <column>
        <name>descripcion</name>
        <type>character varying(100)</type>
        <null>YES</null>
    </column>
    <column>
        <name>id</name>
        <type>integer</type>
        <null>NO</null>
    </column>
    <column>
        <name>idsec</name>
        <type>serial</type>
        <null>NO</null>
        <default>nextval('secuencias_idsec_seq'::regclass)</default>
    </column>
    <column>
        <name>nombre</name>
        <type>character varying(50)</type>
        <null>NO</null>
    </column>
    <column>
        <name>valor</name>
        <type>integer</type>
        <null>YES</null>
    </column>
    <column>
        <name>valorout</name>
        <type>integer</type>
        <null>YES</null>
    </column>
    <indexes>
        <name>secuencias_pkey</name>
        <constraint>PRIMARY KEY (idsec)</constraint>
    </indexes>
    <indexes>
        <name>ca_secuencias_secuenciasejercicios</name>
        <constraint>FOREIGN KEY (id)
            REFERENCES secuenciasejercicios (id)
            ON DELETE CASCADE ON UPDATE CASCADE</constraint>
    </indexes>
</table>