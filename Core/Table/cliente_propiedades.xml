<?xml version="1.0" encoding="UTF-8"?>
<!--
    Document   : cliente_propiedades.xml
    Author     : Carlos Garcia Gomez
    Description:
        Estructura de la tabla cliente_propiedades.
-->
<table>
    <column>
        <name>name</name>
        <type>character varying(50)</type>
        <null>NO</null>
    </column>
    <column>
        <name>codcliente</name>
        <type>character varying(10)</type>
        <null>NO</null>
    </column>
    <column>
        <name>text</name>
        <type>text</type>
    </column>
    <indexes>
        <name>cliente_propiedades_pkey</name>
        <constraint>PRIMARY KEY (name,codcliente)</constraint>
    </indexes>
    <indexes>
        <name>ca_cliente_propiedades_clientes</name>
        <constraint>FOREIGN KEY (codcliente)
            REFERENCES clientes (codcliente)
            ON DELETE CASCADE ON UPDATE CASCADE</constraint>
    </indexes>
</table>