<?xml version="1.0" encoding="UTF-8"?>
<!--
    Document   : fs_roles_access.xml
    Author     : Carlos Garcia Gomez
    Description:
        Estructura de la tabla fs_roles_access.
-->
<table>
    <column>
        <name>id</name>
        <type>serial</type>
        <null>NO</null>
        <default>nextval('fs_roles_access_id_seq'::regclass)</default>
    </column>
    <column>
        <name>codrol</name>
        <type>character varying(20)</type>
        <null>NO</null>
    </column>
    <column>
        <name>pagename</name>
        <type>character varying(30)</type>
        <null>NO</null>
    </column>
    <column>
        <name>allowdelete</name>
        <type>boolean</type>
        <null>NO</null>
        <default>true</default>
    </column>
    <column>
        <name>allowupdate</name>
        <type>boolean</type>
        <null>NO</null>
        <default>true</default>
    </column>
    <indexes>
        <name>fs_roles_access_pkey</name>
        <constraint>PRIMARY KEY (id)</constraint>
    </indexes>
    <indexes>
        <name>fs_roles_access_roles</name>
        <constraint>FOREIGN KEY (codrol) REFERENCES fs_roles (codrol) ON DELETE CASCADE ON UPDATE CASCADE</constraint>
    </indexes>
    <indexes>
        <name>fs_roles_access_page</name>
        <constraint>FOREIGN KEY (pagename) REFERENCES fs_pages (name) ON DELETE CASCADE ON UPDATE CASCADE</constraint>
    </indexes>
    <indexes>
        <name>unique_fs_roles_access</name>
        <constraint>UNIQUE (codrol,pagename)</constraint>
    </indexes>
</table>
